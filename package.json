{
	"name": "aki-portfolio",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"engines": {
		"node": "20.16.0"
	},
	"scripts": {
		"dev": "rm -rf ./.next/**/* && WATCHPACK_POLLING=true next dev -p 34661",
		"dev:scan": "rm -rf ./.next/**/* && env WATCHPACK_POLLING=true SCAN=true next dev -p 34661",
		"build": "npm run lint && rm -rf ./.next/**/* && next build",
		"build-opennext": "opennextjs-cloudflare build",
		"cloudflare:build": "npx @cloudflare/next-on-pages",
		"preview": "next start -p 34661",
		"format": "prettier --write './**/*'",
		"lint": "biome lint"
	},
	"dependencies": {
		"@akimeaki/check-browser": "0.4.0",
		"@akimeaki/null-to-undefined": "1.0.1",
		"@akimeaki/reset-css": "0.6.0",
		"@biomejs/biome": "1.9.4",
		"@kuma-ui/core": "1.5.9",
		"@nanostores/react": "1.0.0",
		"@opennextjs/cloudflare": "1.1.0",
		"@types/luxon": "3.6.2",
		"@types/node": "22.15.29",
		"@types/react": "18.3.12",
		"@types/react-dom": "18.3.1",
		"@types/sanitize-html": "2.16.0",
		"@types/three": "0.177.0",
		"luxon": "3.6.1",
		"nanostores": "1.0.1",
		"next": "15.3.3",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"rough-notation": "0.5.1",
		"sanitize-html": "2.17.0",
		"sass": "1.89.1",
		"shiki": "3.4.2",
		"three": "0.177.0",
		"typescript": "5"
	},
	"devDependencies": {
		"@kuma-ui/next-plugin": "1.3.3",
		"prettier": "3.5.3",
		"prettier-plugin-sh": "0.17.4"
	}
}
